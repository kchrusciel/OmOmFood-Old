web: java -Dspring.profiles.active=prod -Dserver.port=$PORT -Dspring.datasource.url=$JDBC_DATABASE_URL -Dspring.datasource.user=$JDBC_DATABASE_USERNAME -Dspring.datasource.password=$JDBC_DATABASE_PASSWORD -Dspring.social.twitter.appId=$TWITTER_APP_ID -Dspring.social.twitter.appSecret=$TWITTER_APP_SECRET -jar  target/omomfood-0.1.0-SNAPSHOT.jar